@model HospitalManagement.Models.Doctor

@{
    ViewData["Title"] = "Delete Doctor";
}

<h1>Delete Doctor</h1>
<h3>Are you sure you want to delete this doctor?</h3>

<div>
    <h4>@Model.FullName</h4>
    <p>Specialty: @Model..Specialty</p>
    <p>License Number: @Model.LicenseNumber</p>
</div>



<form asp-action="DeleteDoctor" asp-route-id="@Model.Id" method="post">
   
   
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("ManageDoctors", "Admin")" class="btn btn-secondary">Cancel</a>
</form>


